{
  "name": "env-doctor",
  "version": "1.0.0",
  "description": "Intelligent diagnostic and repair tool for JavaScript/TypeScript development environments",
  "main": "dist/cli.js",
  "bin": {
    "env-doctor": "./dist/cli.js"
  },
